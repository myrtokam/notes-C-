using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp20

{
    //Προσθήκη και Αφαίρεση

    class Program
    {
        static void Main(string[] args)
        {


            List<int> name = new List<int> { 1, 2, 3, 4, 5 };

            name.AddRange(new int[] { 10, 20, 30, 40, 50 });
            name.Remove(30);
            foreach (int names in name)
            {
                Console.WriteLine(names);

            }



            Console.ReadKey();

        }




    }
}
