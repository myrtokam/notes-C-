//Μετακίνηση στοιχείων πίνακα:

   // Δημιουργήστε έναν πίνακα με τις τιμές [10, 20, 30, 40, 50].
 //   Μετακινήστε όλα τα στοιχεία του πίνακα κατά μία θέση προς τα δεξιά και το τελευταίο στοιχείο να γίνει το πρώτο.
 //   Εκτυπώστε τον τροποποιημένο πίνακα.

int[] array = { 10, 20, 30, 40, 50 };
int last = array[array.Length - 1];
for (int i = array.Length - 1; i > 0; i--)
{
    array[i] = array[i - 1];
}
array[0] = last;
foreach (int num in array)
{
    Console.Write(num + " ");
}
