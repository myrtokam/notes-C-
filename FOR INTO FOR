public class Programe
{

    public static void Main(string[] args)
    {
        




        for (int i = 0; i <10; i++)
        {
            for (int j = 0; j < 10; j++)
            {
                Console.WriteLine($" i is {i} and  j is {j}");
            }




            Console.WriteLine(" XX");






            Console.ReadKey();
        }
    }
}   

